<%= bootstrap_form_for student, remote: true, authenticity_token: true, data: { type: :script } do |f| %>
  <%= f.text_field :first_name %>
  <%= f.text_field :last_name %>
  <%= f.number_field :age, max: 149 %>
  <%= f.select :degree, ['HS', 'College', 'Masters', 'PhD'] %>
  <%= f.submit "Submit", class: "btn btn-outline-success" %>
<% end %>
