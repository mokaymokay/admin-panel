<tr id="student_<%= student.id %>">
  <td><%= student.id %></td>
  <td><%= student.first_name %></td>
  <td><%= student.last_name %></td>
  <td><%= student.age %></td>
  <td><%= student.degree %></td>
  <td><%= link_to("Details", student_path(student), class: "btn btn-outline-primary btn-sm") %></td>
  <td><%= link_to("Edit", edit_student_path(student), remote: true, class: "btn btn-outline-warning btn-sm") %></td>
  <td><%= link_to "Delete", student_path(student), remote: true, method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-outline-danger btn-sm" %></td>
</tr>
